{
  "name": "@expressjs/perf-wg",
  "version": "1.0.0",
  "description": "ExpressJS Performance Working Group",
  "scripts": {
    "setup": "npm i",
    "runner:build": "npm run build --prefix ./packages/runner-docker",
    "runner:run": "npm run run --prefix ./packages/runner-docker",
    "test:load": "npx expf load --test=@expressjs/perf-load-example",
    "test:load:node": "npx expf load --test=@expressjs/perf-load-node-http",
    "compare": "npx expf compare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expressjs/perf-wg.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "workspaces": [
    "./packages/*",
    "./perf/load/*",
    "./perf/bench/*"
  ]
}
